<p id="notice"><%= notice %></p>

<h1>Lpublishers</h1>

<table>
  <thead>
    <tr>
      <th>Id editore</th>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lpublishers.each do |lpublisher| %>
      <tr>
        <td><%= lpublisher.ID_EDITORE %></td>
        <td><%= lpublisher.Nome %></td>
        <td><%= link_to 'Show', lpublisher %></td>
        <td><%= link_to 'Edit', edit_lpublisher_path(lpublisher) %></td>
        <td><%= link_to 'Destroy', lpublisher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lpublisher', new_lpublisher_path %>
